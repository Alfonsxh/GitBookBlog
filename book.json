{
    "author": "Alfons <alfons_xh@163.com>",
    "description": "记录所学内容",
    "plugins": [
        "disqus",
        "splitter",
        "advanced-emoji",
        "github",
        "expandable-chapters-small",
        "copy-code-button",
        "book-summary-scroll-position-saver",
        "mathjax-update-cdn",
        "anchors",
        "anchor-navigation-ex",
        "tbfed-pagefooter"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "Alfonsxh"
        },
        "github": {
            "url": "https://github.com/Alfonsxh"
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": true,
            "isShowTocTitleIcon": true,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy Alfons 2018",
            "modify_label": "Last modify time:",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}